#!bin/sh

set -e

systemctl enable lightdm.service
systemctl start lightdm.service

echo $DBUS_SESSION_BUS_ADDRESS


env DBUS_SESSION_BUS_ADDRESS=[path] xfconf-query -c xsettings -p /Net/ThemeName -s "Matcha-dark-aliz"
env DBUS_SESSION_BUS_ADDRESS=[path] xfconf-query -c xfwm4 -p /general/theme -s "Matcha-dark-aliz"
env DBUS_SESSION_BUS_ADDRESS=[path] xfconf-query -c xfce4-desktop -p /backdrop/screen0/monitor0/last-image -s /usr/share/images/desktop-base/SecuDevel.jpeg

echo "Ha Funcionado"
